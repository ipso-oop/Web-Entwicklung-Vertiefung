{
  "name": "ollama-chat-assistant",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev:frontend": "vite",
    "dev:backend": "nodemon server.js",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=src/.*\\.test\\.js",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=tests/integration/.*\\.test\\.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "node-fetch": "^3.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "nodemon": "^3.0.2",
    "multer": "^2.0.0",
    "pdf2json": "^2.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.21.4",
    "@vitejs/plugin-react": "^4.0.0",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "vite": "^4.3.9"
  }
}